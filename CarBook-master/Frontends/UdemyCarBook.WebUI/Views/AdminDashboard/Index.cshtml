@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/AdminLayout/Index.cshtml";
}

<!-- start page title -->
<div class="row">
    <div class="col-12">
        <div class="page-title-box d-flex align-items-center justify-content-between">
            <h4 class="mb-0 font-size-18">Dashboard</h4>

            <div class="page-title-right">
                <ol class="breadcrumb m-0">
                    <li class="breadcrumb-item"><a>Dashboard</a></li>

                </ol>
            </div>

        </div>
    </div>
</div>


<!-- ======= Statistic Section ======= -->
@await Component.InvokeAsync("_AdminDashboardStatisticsComponentPartialVC")
<!-- End Statistic Section -->


<div class="row">

    <!-- ======= Chart1 Section ======= -->
    @await Component.InvokeAsync("_AdminDashboardChart1ComponentPartialVC")
    <!-- End Chart1 Section -->

    <!-- ======= Chart2 Section ======= -->
    @await Component.InvokeAsync("_AdminDashboardChart2ComponentPartialVC")
    <!-- End Chart2 Section -->

    <!-- ======= Chart3 Section ======= -->
    @await Component.InvokeAsync("_AdminDashboardChart3ComponentPartialVC")
    <!-- End Chart3 Section -->

    
</div>

<div class="row">

    <!-- ======= CarPricing Section ======= -->
    @await Component.InvokeAsync("_AdminDashboardCarPricingListComponentPartialVC")
    <!-- End CarPricing Section -->

    <!-- ======= BlogList Section ======= -->
    @await Component.InvokeAsync("_AdminDashboardBlogListComponentPartialVC")
    <!-- End BlogList Section -->

</div>



